{
  "name": "@tradespro/core-engine",
  "version": "1.0.0",
  "description": "TradesPro Core Calculation Engine - Plugin System and Shared Interfaces",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "browser": "./src/index.browser.ts",
      "require": "./dist/index.js",
      "import": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./plugins": {
      "require": "./dist/plugins/index.js",
      "import": "./src/plugins/index.ts",
      "types": "./dist/plugins/index.d.ts",
      "default": "./src/plugins/index.ts"
    },
    "./tableLookups": {
      "require": "./dist/tableLookups.js",
      "import": "./src/tableLookups.ts",
      "types": "./dist/tableLookups.d.ts",
      "default": "./src/tableLookups.ts"
    },
    "./src/*": "./src/*",
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "dev": "tsc --watch",
    "generate-keys": "node dist/tools/generateKeys.js",
    "package-plugin": "node dist/tools/pluginPackager.js",
    "verify-plugin": "node dist/tools/verifyPlugin.js",
    "test-sandbox": "node dist/tools/testSandbox.js",
    "health-check": "node dist/tools/healthCheckCLI.js",
    "plugin-manager": "node dist/tools/pluginManagerCLI.js",
    "dependency-check": "node dist/tools/dependencyCheckerCLI.js"
  },
  "keywords": [
    "tradespro",
    "core-engine",
    "plugin-system",
    "calculation-engine"
  ],
  "author": "TradesPro Team",
  "license": "MIT",
  "optionalDependencies": {
    "vm2": "^3.9.19",
    "tar": "^7.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}




